{
  "name": "sr-bot",
  "version": "1.0.1",
  "description": "Automatically updates Overwatch players SRs from config",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "start": "npm run build && node dist/index.js start",
    "start:generate": "npm run build && node dist/index.js generate",
    "start:static": "node dist/index.js",
    "check": "eslint src --ext .ts",
    "fix": "eslint src --ext .ts --fix",
    "test": "mocha -r ts-node/register ./src/test/*.ts"
  },
  "bin": {
    "sr-bot": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "author": "Derek Sims (SCRAWCHIMARI#1144)",
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "colors": "^1.3.3",
    "commander": "^2.19.0",
    "discord.js": "^11.4.2",
    "libsodium-wrappers": "^0.7.3",
    "overwatch-api": "^0.7.5",
    "uws": "^9.14.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.12.2",
    "@typescript-eslint/parser": "^1.5.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gts": "^0.9.0",
    "mocha": "^6.0.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3",
    "typescript": "~3.3.3333"
  },
  "pre-commit": [
    "check"
  ]
}
